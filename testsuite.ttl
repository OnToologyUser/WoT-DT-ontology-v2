@prefix : <http://example.org/ns#> .
@prefix dtw: <https://w3id.org/def/dtw#> .
@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vtc: <https://w3id.org/def/vtc#> .

# WOTDT-1: DigitalTwin is subclass of td:Thing
:Test_WOTDT_1 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwin SubClassOf td:Thing" .

# WOTDT-3-4: exactly one PhysicalEntity per DigitalTwin
:Test_WOTDT_3_4 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwin SubClassOf dtw:hasPhysicalEntity exactly 1 dtw:PhysicalEntity" .

# WOTDT-5-7: at least one DigitalEntity per DigitalTwin
:Test_WOTDT_5_6_7 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwin SubClassOf dtw:hasDigitalEntity some dtw:DigitalEntity" .

# WOTDT-8-10: DigitalEntity has various types of Models
:Test_WOTDT_8_9_10 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalEntity SubClassOf dtw:hasModels some dtw:BehavioralModel and dtw:hasModels some dtw:PhysicalModel and dtw:hasModels some dtw:GeometricModel and dtw:hasModels some dtw:RulesModel and dtw:hasModels some dtw:SemanticModel" .

# WOTDT-11: Model has Format with hctl target and extension
:Test_WOTDT_11 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:Model SubClassOf dtw:hasFormat some dtw:Format and dtw:Format SubClassOf hctl:hasTarget some and dtw:Format SubClassOf dcterms:hasExtension some" .

# WOTDT-12-13: DigitalEntity has Data resources
:Test_WOTDT_12_13 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalEntity SubClassOf dtw:hasData some dcat:Resource" .

# WOTDT-14-15: DigitalEntity contains InteractionAffordances
:Test_WOTDT_14_15 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalEntity SubClassOf dtw:containsInteractionAffordance some td:InteractionAffordance" .

# WOTDT-16-18: DigitalTwin has Connections of various types
:Test_WOTDT_16_17_18 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwin SubClassOf dtw:hasConnection some dtw:ModelModelConnection and dtw:hasConnection some dtw:ModelDataConnection and dtw:hasConnection some dtw:ModelServiceConnection and dtw:hasConnection some dtw:DataServiceConnection" .

# WOTDT-19: ConnectionPoint subclasses
:Test_WOTDT_19 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:ConnectionPoint SubClassOf dtw:InternalConnectionPoint and dtw:ConnectionPoint SubClassOf dtw:ExternalConnectionPoint" .

# WOTDT-20: Connection has provider and consumer
:Test_WOTDT_20 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:Connection SubClassOf dtw:hasProvider some dtw:ConnectionPoint and dtw:Connection SubClassOf dtw:hasConsumer some dtw:ConnectionPoint" .

# WOTDT-21: Resources, Models, Affordances have connection points
:Test_WOTDT_21 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:Model SubClassOf dtw:hasConnectionPoint some dtw:ConnectionPoint and dcat:Resource SubClassOf dtw:hasConnectionPoint some dtw:ConnectionPoint and td:InteractionAffordance SubClassOf dtw:hasConnectionPoint some dtw:ConnectionPoint" .

# WOTDT-22: DigitalTwinInstance subclass
:Test_WOTDT_22 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwinInstance SubClassOf dtw:DigitalTwin" .

# WOTDT-23: DigitalTwinAggregate subclass and aggregation relations
:Test_WOTDT_23 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:DigitalTwinAggregate SubClassOf dtw:DigitalTwin and dtw:DigitalTwinAggregate SubClassOf dtw:aggregateDTwI some dtw:DigitalTwinInstance and dtw:DigitalTwinAggregate SubClassOf dtw:aggregateDTwA some dtw:DigitalTwinAggregate" .

# WOTDT-24: Model aggregated from others
:Test_WOTDT_24 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dtw:Model SubClassOf dtw:modelAggregatedFrom some dtw:Model" .

# WOTDT-25: Data resources aggregated from others
:Test_WOTDT_25 a vtc:TestCaseDesign;
    vtc:desiredBehaviour "dcat:Resource SubClassOf dtw:resourceAggregatedFrom some dcat:Resource" .
